<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">

	<ViewSettingsDialog
		id="viewSettingsDialog"
		confirm=".onConfirmViewSettingsDialog">
		<sortItems>
			<ViewSettingsItem
				text="{i18n>masterSort1}"
				key="Name"/>
			<ViewSettingsItem
				text="{i18n>masterSort2}"
				key="ProductId"
                selected="true"/>
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>